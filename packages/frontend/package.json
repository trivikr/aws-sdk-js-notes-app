{
  "name": "@aws-sdk-todo-app/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for todo app created using modular AWS SDK for JavaScript",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.4.2-ci.0",
    "@aws-sdk/client-s3": "3.4.2-ci.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.4.2-ci.0",
    "@aws-sdk/s3-request-presigner": "3.4.2-ci.0",
    "@aws-sdk/util-create-request": "3.4.2-ci.0",
    "@aws-sdk/util-format-url": "3.4.2-ci.0",
    "@reach/router": "1.3.4",
    "react": "17.0.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "17.0.1"
  },
  "scripts": {
    "prepare:frontend": "cd .. && yarn prepare:frontend",
