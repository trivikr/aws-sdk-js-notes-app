    "@types/node": "^14.14.2",
    "@types/reach__router": "1.3.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/snowpack-env": "^2.3.2",
    "local-web-server": "^4.2.1",
    "snowpack": "^3.0.1",
